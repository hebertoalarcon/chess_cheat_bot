{

    "manifest_version": 2,
    "name": "Borderify",
    "version": "1.0",
  
    "description": "Extract moves in a SAN format chess",
  
    "icons": {
      "48": "icons/border-48.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://*.chess.com/*"],
        "js": ["borderify.js"]
      }
    ],
    "permissions": [
      "http://localhost/*"
    ]
  
  }
  